<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef Abstract="True" Name="EggFertAvesome" ParentName="EggBase" >
        <tickerType>Normal</tickerType>
        <ingestible>
            <preferability>DesperateOnly</preferability>
        </ingestible>
        <thingCategories>
             <li>EggsFertilized</li>
        </thingCategories>
         <comps>
            <li Class="CompProperties_TemperatureRuinable">
            <minSafeTemperature>-1200</minSafeTemperature>
            <maxSafeTemperature>1200</maxSafeTemperature>
            <progressPerDegreePerTick>0.00003</progressPerDegreePerTick>
         </li>
         </comps>
     </ThingDef>

  <!-- PotatoKingEgg -->
  
  <ThingDef ParentName="EggFertAvesome">
	  
    <defName>FR_PotatoKingEgg</defName>
    <label>Potatoking grain</label>
    <description>"Seeds" of the Potato King. Weather conditions will not affect "germination". If intentionally not destroyed, there is a near-zero chance that it will happen on its own. Highly nutritious, but there is a possibility of food poisoning. Recycling is recommended for population control.</description>
    <possessionCount>1</possessionCount>
    <graphicData>
      <texPath>Things/Item/Resource/AnimalProductRaw/FloranEgg</texPath>
      <color>(184, 134, 11)</color>
		<drawSize>1</drawSize>
    </graphicData>
	  <statBases>
		  <Mass>1.15</Mass>
		  <MaxHitPoints>120</MaxHitPoints>
		  <DeteriorationRate>0.3</DeteriorationRate>
		  <Flammability>0</Flammability>
		  <Nutrition>2</Nutrition>
		  <FoodPoisonChanceFixedHuman>0.1</FoodPoisonChanceFixedHuman>
	      <MarketValue>14.0</MarketValue>
    </statBases>
    <comps>
		<li Class="CompProperties_Hatcher">
			<hatcherDaystoHatch>6</hatcherDaystoHatch>
			<hatcherPawn>FR_PotatoKing</hatcherPawn>
		</li>
	  </comps>
  </ThingDef>

